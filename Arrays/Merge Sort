#include<bits/stdc++.h>
using namespace std;

void merge(vector<int> & A, int m, vector<int> & B, int n){
    int idx = m+n-1, i = m-1, j = n-1;
    while( i>=0 && j>=0){
        if(A[i] >= B[j]){
            A[idx--] = A[i--];   // A bda h to A ko -- krenge
        }else{
            A[idx--] = B[j--];   //B bda h to B ko -- krenge
        }
    }
    while(j >=0){
        A[idx--] = B[j--];
    }
}